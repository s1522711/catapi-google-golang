        "id": 1,
        "uuid": "google-photos-api",
        "name": "Google Photos Album API",
        "author": "s1522711",
        "description": "A Go API that serves random images from a public Google Photos album",
        "features": [
            "google-photos-api"
        ],
        "docker_images": {
            "google-photos-api": "golang:1.21-alpine"
        },
        "file_denylist": [],
        "startup": "git pull origin main && go run main.go",
        "config": {
            "files": "{\r\n    \"config.json\": {\r\n        \"parser\": \"file\",\r\n        \"find\": {\r\n            \"album_url\": \"https://photos.app.goo.gl/YOUR_ALBUM_ID\",\r\n            \"port\": 8080\r\n        }\r\n    }\r\n}",
            "startup": "{\r\n    \"config.json\": {\r\n        \"parser\": \"file\",\r\n        \"find\": {\r\n            \"album_url\": \"https://photos.app.goo.gl/YOUR_ALBUM_ID\",\r\n            \"port\": 8080\r\n        }\r\n    }\r\n}",
            "logs": "{}",
            "stop": "^C"
        },
        "startup_command": "git pull origin main && go run main.go",
        "script": {
            "privileged": false,
            "install": "#!/bin/ash\n# Clone or update the repository from GitHub\nif [ -d .git ]; then\n    git pull origin main\nelse\n    git clone $GITHUB_REPO .\nfi\n# Install dependencies\ngo mod download\ngo mod tidy",
            "entry": "ash",
            "container": "golang:1.21-alpine",
            "extends": null
        },
        "variables": [
            {
                "id": 1,
                "egg_id": 1,
                "name": "GitHub Repository",
                "description": "GitHub repository URL to clone the latest version from",
                "env_variable": "GITHUB_REPO",
                "default_value": "https://github.com/s1522711/catapi-google-golang.git",
                "user_viewable": true,
                "user_modifiable": true,
                "rules": "required|string|max:500",
                "field_type": "text"
            },
            {
                "id": 2,
                "egg_id": 1,
                "name": "Album URL",
                "description": "The public Google Photos album URL to fetch images from",
                "env_variable": "ALBUM_URL",
                "default_value": "https://photos.app.goo.gl/YOUR_ALBUM_ID",
                "user_viewable": true,
                "user_modifiable": true,
                "rules": "required|string|max:500",
                "field_type": "text"
            },
            {
                "id": 3,
                "egg_id": 1,
                "name": "Port",
                "description": "The port the API will run on",
                "env_variable": "PORT",
                "default_value": "8080",
                "user_viewable": true,
